{
  "name": "Trello",
  "active": true,
  "default": true,
  "connector": "RequestsGet",
  "url": "https://api.trello.com/1",
  "query_template": "{url}?q={query_string}",
  "query_processors": ["AdaptiveQueryProcessor"],
  "query_mappings": "",
  "result_processors": [
    "MappingResultProcessor",
    "LenLimitingResultProcessor",
    "CosineRelevancyResultProcessor"
  ],
  "response_mappings": "RESULTS=cards",
  "result_mappings": "title=name,body=desc,url=url,author=username,date_published=dateLastActivity",
  "credentials": "bearer=<your-trello-token>",
  "tags": ["Trello"]
}
