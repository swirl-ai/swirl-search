{
    "name": "Algolia Search",
    "active": false,
    "default": false,
    "connector": "RequestsPost",
    "url": "https://<Application-ID>-dsn.algolia.net/1/indexes/{indexName}/query",
    "method": "POST",
    "body": "{ \"params\": \"query={query_string}&hitsPerPage=10\" }",
    "query_processors": [
        "AdaptiveQueryProcessor"
    ],
    "result_processors": [
        "MappingResultProcessor",
        "LenLimitingResultProcessor",
        "CosineRelevancyResultProcessor"
    ],
    "response_mappings": "FOUND=nbHits,RESULTS=hits",
   "result_mappings": "title=hits.name,body=hits._snippetResult.bio.value,date_published=hits.year,url=NO_PAYLOAD",
    "results_per_query": 10,
    "headers": {
        "X-Algolia-Application-Id": "<Your-Algolia-Application-Id>",
        "X-Algolia-API-Key": "<Your-Algolia-API-Key>"
    },
    "tags": [
        "Algolia",
        "Search",
        "External"
    ]
}
